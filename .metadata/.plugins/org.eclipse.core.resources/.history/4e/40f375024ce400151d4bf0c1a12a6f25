package springtest2;

import java.util.Date;

public class Ticket {
	private Event event;
	private Date date;
	private boolean isVip;
	
	public void setDate(Date date){
		this.date = date;
	}
	
	public void setEvent(Event event){
		this.event = event;
	}
	
	public Date getDate(){
		return date;
	}

	public Event getEvent(){
		return event;
	}
	
	public boolean getIsVip(){
		return isVip;
	}
	
	public void setIsVip(boolean isVip){
		this.isVip = isVip;
	}
	
	public Ticket(Date date, Event event, boolean isVip){
		
	}
	
	
}
