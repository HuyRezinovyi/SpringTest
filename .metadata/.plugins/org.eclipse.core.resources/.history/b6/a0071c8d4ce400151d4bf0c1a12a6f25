package springtest2;

import java.util.Date;

public class Ticket {
	private Event event;
	private Date date;
	private Auditorium auditorium;
	private boolean isVip;
	
	public Date getDate(){
		return date;
	}

	public Event getEvent(){
		return event;
	}
	
	public boolean getIsVip(){
		return isVip;
	}
	
	public Auditorium getAuditorium(){
		return auditorium;
	}

	public Ticket(Date date, Event event, boolean isVip){
		this.date = date;
		this.event = event;
		this.isVip = isVip;
		this.auditorium = auditorium;
	}
	
}
